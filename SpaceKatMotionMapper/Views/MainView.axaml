<UserControl
    d:DesignHeight="600"
    d:DesignWidth="400"
    mc:Ignorable="d"
    x:Class="SpaceKatMotionMapper.Views.MainView"
    x:DataType="vm:MainViewModel"
    xmlns="https://github.com/avaloniaui"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:view="clr-namespace:SpaceKatMotionMapper.Views"
    xmlns:vm="clr-namespace:SpaceKatMotionMapper.ViewModels"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Design.DataContext>
        <vm:MainViewModel />
    </Design.DataContext>

    <Grid RowDefinitions="Auto, *">
        <view:ListeningInfoView Grid.Row="0" />
        <view:ConfigCenterView Grid.Row="1" />
    </Grid>

</UserControl>